.nav{
    width:100%;
    height:70px;
    position: fixed;
    left:0;
    top:0;
    background-color: #fff;
}
.midnav{
    width:1000px;
    height:70px;
    margin:0 auto;
}
.midnav span{
    width:182px;
    height:70px;
    display: block;
    float:left;
}
.midnav ul{
    width:180px;
    height:70px;
    display: block;
    float:right;

}
.midnav ul li{
    margin-top:24px;
    padding-right:12px;
    padding-left:12px;
    height:28px;
    text-align: center;
    line-height: 28px;
    display: inline-block;
    font-size:20px;
    border-right:1px solid #d1d1d1;
}
.midnav ul li:nth-child(2) a{
    color:#19783e;
}
.midnav ul li:nth-child(2){
    border:none;
    padding-right: 0;
}
.article{
    width:100%;
    height:100%;
    margin-top:90px;
}
.articleBlock{
    margin:0 auto;
    width:1000px;
    height:100%;
    box-shadow: 0px 2px #666;
    border:1px solid #666;
    background-color: #fff;

}.articleBlock ul li{
    display:block;
    padding-top:20px;
    height:140px;
    border-bottom:1px dashed #999;
 }
.articleBlock .page2{
    display: none;
}
.articleBlock ul li:nth-child(5){
    border:none;
}
.articleBlock ul li p a{
    display: block;
    width:1000px;
    height:40px;
    font-size:26px;
    color:#19783e;
}
.articleBlock ul li p:nth-child(2){
    color:#666;
    font-size:18px;
    line-height:26px;
    height:100px;
    width:700px;
    display: block;
    float:left;
    text-indent: 36px;
}
.articleBlock ul li span{
    display: block;
    float:right;
    width:250px;
    height:100px;
}
.articleBlock ul li span img{
    display: block;
    width:60%;
    height:100%;
}
